{
    "doctype": "DocType",
    "name": "Report Subscription",
    "module": "TEMS Insights",
    "custom": 0,
    "istable": 0,
    "engine": "InnoDB",
    "field_order": [
        "report_name",
        "frequency",
        "recipient_emails",
        "active"
    ],
    "fields": [
        {
            "fieldname": "report_name",
            "fieldtype": "Data",
            "label": "Report Name",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "frequency",
            "fieldtype": "Select",
            "label": "Frequency",
            "options": "Daily\nWeekly\nMonthly",
            "default": "Weekly"
        },
        {
            "fieldname": "recipient_emails",
            "fieldtype": "Small Text",
            "label": "Recipient Emails"
        },
        {
            "fieldname": "active",
            "fieldtype": "Check",
            "label": "Active",
            "default": "1"
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        },
        {
            "role": "TEMS Executive",
            "read": 1
        },
        {
            "role": "Operations Manager",
            "read": 1
        }
    ]
}