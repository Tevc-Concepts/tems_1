{
    "doctype": "DocType",
    "name": "Field Service Request",
    "module": "TEMS CRM",
    "engine": "InnoDB",
    "field_order": [
        "customer",
        "address",
        "request_type",
        "description",
        "priority",
        "status",
        "assigned_to"
    ],
    "fields": [
        {
            "fieldname": "customer",
            "label": "Customer",
            "fieldtype": "Link",
            "options": "Customer",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "address",
            "label": "Address",
            "fieldtype": "Link",
            "options": "Address"
        },
        {
            "fieldname": "request_type",
            "label": "Request Type",
            "fieldtype": "Data"
        },
        {
            "fieldname": "description",
            "label": "Description",
